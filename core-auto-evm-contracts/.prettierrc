{
  "printWidth": 140,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "semi": true,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-solidity"],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "compiler": "0.8.24",
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": false,
        "parser": "solidity-parse"
      }
    }
  ]
}
